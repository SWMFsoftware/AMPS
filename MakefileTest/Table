Name=test/batl-reader
Time=2

Ref=1{pgcc}@@(0+pleiades)
Ref=1{icc}@@(0+pleiades)
Ref=1{gcc}@@(0+pleiades)

Compile=
>>>@echo "test_batl-reader_compile..." > test_batl-reader.diff 

<<<ifeq ($(wildcard BATL/*),)
>>>echo "Installing BATL....."
>>>git clone --depth 1 git@github.com:SWMFsoftware/BATL
<<<endif

<<<ifeq ($(COMPILE.c),icc)
>>>cd BATL;./Config.pl -install -compiler=ifort,iccmpicxx -g=4,4,4 -r=2,2,2 -ng=0 
<<<endif

<<<ifeq ($(COMPILE.c),gcc)
>>>cd BATL;./Config.pl -install -compiler=gfortran,gcc_mpicc -g=4,4,4 -r=2,2,2 -ng=0  
<<<endif

<<<ifeq ($(COMPILE.c),pgcc)
>>>cd BATL;./Config.pl -install -compiler=pgf90,pgccmpicxx -g=4,4,4 -r=2,2,2 -ng=0  
<<<endif

>>>cd BATL;make READAMRLIB

>>>@cd BATL; ./Config.pl -install -compiler=${FORTRAN_COMPILER_NAME},${C_COMPILER_NAME} -g=4,4,4 -r=2,2,2 -ng=0; make READAMRLIB 
>>>@echo "Install AMPS......"  
>>>./Config.pl -application=test/BATL-reader -spice-path=nospice -batl-path=${PTDIR}/BATL -swmf-path=${PTDIR}/SWMF -cplr-data-path=${PTDIR}/data/input/DataFileReader/BATL  
>>>make
>>>@echo "test_batl-reader_compile... done" >> test_batl-reader.diff
