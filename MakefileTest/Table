>>>@echo "test_AMPS-BATSRUS-PIC-coupling_check..." >> test_AMPS-BATSRUS-PIC-coupling.diff

<<<ifneq ($(wildcard SWMF/test12_aniso_AMPS_dynamic_gmpc.diff),)
>>>@cp SWMF/test12_aniso_AMPS_dynamic_gmpc.diff test_AMPS-BATSRUS-PIC-coupling--gmpc-dynamics.diff
<<<endif

<<<ifneq ($(wildcard SWMF/test12_aniso_AMPS_gmpc.diff),)
>>>@cp SWMF/test12_aniso_AMPS_gmpc.diff test_AMPS-BATSRUS-PIC-coupling--gmpc.diff
<<<endif

<<<ifneq ($(wildcard SWMF/test12_aniso_AMPS_openmp_gmpc.diff),)
