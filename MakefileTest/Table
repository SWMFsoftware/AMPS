<<<endif

<<<ifneq ($(wildcard SWMF/test12_aniso_AMPS_divE_corr_gmpc.diff),)
>>>@cp SWMF/test12_aniso_AMPS_divE_corr_gmpc.diff test_AMPS-BATSRUS-PIC-coupling--gmpc-divE_cor.diff
<<<endif

<<<ifneq ($(wildcard SWMF/test12_aniso_AMPS*.diff),)
