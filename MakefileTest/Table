>>>./Config.pl -application=test/magnetic-reconnection -amps-test=on ${TESTmagnetic-reconnectionKEYS}
>>>@rm -rf srcTemp
>>>-@($(MAKE) -j8  amps)
>>>@echo "test_magnetic-reconnection_compile... done" >> test_magnetic-reconnection$(TESTmagnetic-reconnection-REF).diff

Check=
>>>@echo "test_magnetic-reconnection_check..." >> test_magnetic-reconnection$(TESTmagnetic-reconnection-REF).diff
