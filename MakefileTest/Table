>>>output/test_Periodic-BC/test3-wave50.dat.Reduced.Step=10.ref_np`ls ${TESTPeriodic-BCDIR}/PT/thread* |wc -l |tr -d ' '`  >> test_Periodic-BC--test=3-wave.diff

>>>@ls -l test_Periodic-BC--test=0-wave.diff
>>>@ls -l test_Periodic-BC--test=1-wave.diff
>>>@ls -l test_Periodic-BC--test=2-wave.diff
>>>@ls -l test_Periodic-BC--test=3-wave.diff

>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTPeriodic-BCDIR}/PT/plots/amps.TrajectoryTracking.out=19.s=0.H2O.dat.Reduced.Step=10  \
