>>>-(${SCRIPTDIR}/DiffNum.pl -a=2.0E-14 SWMF-IH/SWMF/run_test/all.AMPS.FieldLines.out=6.dat output/test_SEP3D/SC-IH--FL--PT/droge_diffusion_Qin.all.AMPS.FieldLines.out=6.dat  > test_SEP-SC-IH--FL--FL-attachment--Droge_diffusion_Qin.diff)

>>>@cat SWMF-IH/SWMF/run_test/amps-out.thread=*/sample.density.field-line=0.r=2.000000e-01.dat > SWMF-IH/SWMF/run_test/all.sample.density.field-line=0.r=2.000000e-01.dat
>>>-(${SCRIPTDIR}/DiffNum.pl -a=2.0E-14 SWMF-IH/SWMF/run_test/all.sample.density.field-line=0.r=2.000000e-01.dat output/test_SEP3D/SC-IH--FL--PT/droge_diffusion_Qin.all.sample.density.field-line=0.r=2.000000e-01.dat  > test_SEP-SC-IH--FL--FL-attachment--Droge_diffusion_Qin.density.diff)

>>>@cat SWMF-IH/SWMF/run_test/amps-out.thread=*/sample.flux.field-line=0.r=2.000000e-01.dat > SWMF-IH/SWMF/run_test/all.sample.flux.field-line=0.r=2.000000e-01.dat
>>>-(${SCRIPTDIR}/DiffNum.pl -a=2.0E-14 SWMF-IH/SWMF/run_test/all.sample.flux.field-line=0.r=2.000000e-01.dat output/test_SEP3D/SC-IH--FL--PT/droge_diffusion_Qin.all.sample.flux.field-line=0.r=2.000000e-01.dat  > test_SEP-SC-IH--FL--FL-attachment--Droge_diffusion_Qin.flux.diff)

>>>@cat SWMF-IH/SWMF/run_test/amps-out.thread=*/sample.return_flux.field-line=0.r=2.000000e-01.dat > SWMF-IH/SWMF/run_test/all.sample.return_flux.field-line=0.r=2.000000e-01.dat
>>>-(${SCRIPTDIR}/DiffNum.pl -a=2.0E-14 SWMF-IH/SWMF/run_test/all.sample.return_flux.field-line=0.r=2.000000e-01.dat output/test_SEP3D/SC-IH--FL--PT/droge_diffusion_Qin.all.sample.return_flux.field-line=0.r=2.000000e-01.dat  > test_SEP-SC-IH--FL--FL-attachment--Droge_diffusion_Qin.return_flux.diff)


>>>@ls -l test_SEP-SC-IH--FL--FL-attachment--Droge_diffusion_Qin.density.diff
>>>@ls -l test_SEP-SC-IH--FL--FL-attachment--Droge_diffusion_Qin.flux.diff
