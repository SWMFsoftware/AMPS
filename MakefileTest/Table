Name=test/COPS@@(0+) 
Time=20 

Ref=2{icc}@@(0+valeriy)
Ref=1{gcc}@@(0+valeriy)
Ref=8{pgcc}@@(0+valeriy)

Ref=5{gcc}@@(0+pleiades)
Ref=6{icc}@@(0+pleiades)
Ref=7{pgcc}@@(0+pleiades)

Keys=-cplr-data-path=/Users/vtenishe/AMPS_DATA_TEST/COPS -model-data-path=/Users/vtenishe/AMPS_DATA_TEST/COPS@@(0+valeriy)
Keys=-cplr-data-path=/nobackupp8/vtenishe/AMPS_DATA_TEST/COPS -model-data-path=/nobackupp8/vtenishe/AMPS_DATA_TEST/COPS@@(0+pleiades) 
Keys=-cplr-data-path=/scratch/02452/vtenishe/AMPS_DATA_TEST/COPS -model-data-path=/scratch/02452/vtenishe/AMPS_DATA_TEST/COPS@@(0+stampede)
Keys=-cplr-data-path=/home/vtenishe/AMPS_DATA_TEST/COPS -model-data-path=/home/vtenishe/AMPS_DATA_TEST/COPS@@(0+amps-gpu+passion)

Check=
>>>@echo "test_COPS_check..." >> test_COPS$(TESTCOPS-REF).diff

>>>-@${TESTCOPSDIR}/../utility/ReduceFile.pl ${TESTCOPSDIR}/PT/plots/pic.H2O.s=0.out=13.dat 10
>>>-@${TESTCOPSDIR}/../utility/RemoveColumn.pl -c=5 ${TESTCOPSDIR}/PT/plots/pic.H2O.s=0.out=13.dat.Reduced.Step=10 
>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTCOPSDIR}/PT/plots/pic.H2O.s=0.out=13.dat.Reduced.Step=10 ${PTDIR}/../../../AMPS_DATA_TEST/COPS/output/pic.H2O.s=0.out=13.dat.Reduced.Step=10$(TESTCOPS-REF).ref_np`ls $(TESTCOPSDIR)/PT/thread* |wc -l |tr -d ' '`  > test_COPS-Volume$(TESTCOPS-REF).diff

>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTCOPSDIR}/PT/plots/SurfaceTriangulation-shadow.dat ${PTDIR}/../../../AMPS_DATA_TEST/COPS/output/SurfaceTriangulation-shadow.dat.ref_np`ls $(TESTCOPSDIR)/PT/thread* |wc -l |tr -d ' '`  > test_COPS-Triangulation-shadow.diff

>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTCOPSDIR}/PT/plots/SurfaceTriangulation.dat ${PTDIR}/../../../AMPS_DATA_TEST/COPS/output/SurfaceTriangulation.dat.ref_np`ls $(TESTCOPSDIR)/PT/thread* |wc -l |tr -d ' '`  > test_COPS-Triangulation.diff

>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTCOPSDIR}/PT/plots/amps.cut-cell.surface-data.out=13.dat ${PTDIR}/../../../AMPS_DATA_TEST/COPS/output/amps.cut-cell.surface-data.out=13.dat$(TESTCOPS-REF).ref_np`ls $(TESTCOPSDIR)/PT/thread* |wc -l |tr -d ' '`  > test_COPS-Surface-Data$(TESTCOPS-REF).diff


>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTCOPSDIR}/PT/plots/test_Orbiter.dat ${PTDIR}/../../../AMPS_DATA_TEST/COPS/output/test_Orbiter.dat$(TESTCOPS-REF).ref_np`ls $(TESTCOPSDIR)/PT/thread* |wc -l |tr -d ' '`  > test_COPS-Particle-Data$(TESTCOPS-REF).diff

>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTCOPSDIR}/PT/plots/test_Orbiter_Drag_Coefficient.dat ${PTDIR}/../../../AMPS_DATA_TEST/COPS/output/test_Orbiter_Drag_Coefficient.dat$(TESTCOPS-REF).ref_np`ls $(TESTCOPSDIR)/PT/thread* |wc -l |tr -d ' '`  > test_COPS-Drag-Coefficient$(TESTCOPS-REF).diff

>>>@cat test_COPS-Volume$(TESTCOPS-REF).diff test_COPS-Triangulation-shadow.diff test_COPS-Triangulation.diff test_COPS-Surface-Data$(TESTCOPS-REF).diff test_COPS-Particle-Data$(TESTCOPS-REF).diff test_COPS-Drag-Coefficient$(TESTCOPS-REF).diff > test_COPS$(TESTCOPS-REF).diff

>>>@ls -l test_COPS-Volume$(TESTCOPS-REF).diff 
>>>@ls -l test_COPS-Triangulation-shadow.diff 
>>>@ls -l test_COPS-Triangulation.diff
>>>@ls -l test_COPS-Surface-Data$(TESTCOPS-REF).diff 
>>>@ls -l test_COPS-Particle-Data$(TESTCOPS-REF).diff 
>>>@ls -l test_COPS-Drag-Coefficient$(TESTCOPS-REF).diff  
>>>@ls -l test_COPS$(TESTCOPS-REF).diff
>>>@echo Test test_COPS has finished: `date`

