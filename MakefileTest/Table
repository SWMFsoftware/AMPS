>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTX-WingDIR}/PT/plots/amps.cut-cell.surface-data.out=2.dat.Reduced.Step=100 ${PTDIR}/../../../AMPS_DATA_TEST/X-WING/output/amps.cut-cell.surface-data.out=2.dat.Reduced.Step=100$(TESTX-Wing-REF).ref_np`ls $(TESTX-WingDIR)/PT/thread* |wc -l |tr -d ' '`  > test_X-Wing-Surface-Sample$(TESTX-Wing-REF).diff

