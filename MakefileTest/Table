Name=LOS@@(0+) 
Time=4
Compile=
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --tmpDir ${PTDIR}/utility/LOS/Tmp

Rundir=
>>>@echo "test_LOS_rundir..." >> test_LOS.diff
>>>@echo "test_LOS_rundir... done" >> test_LOS.diff

Run=
>>>@echo "test_LOS_run..." >> test_LOS.diff
>>>@echo Test test_LOS has started: `date`
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --kernelFile /Users/vtenishe/AMPS_DATA_TEST/LOS/kernels.tm
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --meshFile /Users/vtenishe/AMPS_DATA_TEST/LOS/SHAP5_stefano.ply
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --meshFileShadow /Users/vtenishe/AMPS_DATA_TEST/LOS/SHAP5_shadow.ply
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --dataFile /Users/vtenishe/AMPS_DATA_TEST/LOS/pic.DUST%0.s=2.out=11.dat
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --doCheckShadow yes
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --pltLevels 30
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --pltColorMap afmhot
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --pltBlankBody yes 
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --pltAdditionalBorderPx 7
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --doPlotResult yes
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --spicelib /Users/vtenishe/SPICE/Toolkit/cspice/lib
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --clib /Users/vtenishe/AMPS_DATA_TEST/LOS/cprog.c
>>>cd ${PTDIR}/utility/LOS/ && echo "pltLevels:30" >> .userSettings.conf
>>>cd ${PTDIR}/utility/LOS/ && echo "pltColorMap:afmhot" >> .userSettings.conf
>>>cd ${PTDIR}/utility/LOS/ && echo "pltBlankBody:yes" >> .userSettings.conf
>>>cd ${PTDIR}/utility/LOS/ && echo "pltAdditionalBorderPx:7" >> .userSettings.conf
>>>cd ${PTDIR}/utility/LOS/ && echo "doPlotResult:no" >> .userSettings.conf

>>>#run test case with dust with shadow 
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --doCheckShadow yes
>>>cd ${PTDIR}/utility/LOS/src/ && julia main.jl 2015-02-11T00:00:00 TEST 
>>>cd ${PTDIR}/utility/LOS/Tmp/output && cat *.dat > test_LOS-dust--shadow=yes.res
>>>cd ${PTDIR}/utility/LOS/Tmp/output && rm *.dat 

>>>#run dust without shadow
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --doCheckShadow no
>>>cd ${PTDIR}/utility/LOS/src/ && julia main.jl 2015-02-11T00:00:00 TEST
>>>cd ${PTDIR}/utility/LOS/Tmp/output && cat *.dat > test_LOS-dust--shadow=no.res
>>>cd ${PTDIR}/utility/LOS/Tmp/output && rm *.dat

>>>#run gas case with shadow
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --dataFile /Users/vtenishe/AMPS_DATA_TEST/LOS/pic.H2O.s=0.out=11.dat
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --clib ${PTDIR}/utility/LOS/src/cprog.c

>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --doCheckShadow yes
>>>cd ${PTDIR}/utility/LOS/src/ && julia main.jl 2015-02-11T00:00:00 TEST
>>>cd ${PTDIR}/utility/LOS/Tmp/output && cat *.dat > test_LOS-gas--shadow=yes.res
>>>cd ${PTDIR}/utility/LOS/Tmp/output && rm *.dat

>>>#run gas case without shadow
>>>cd ${PTDIR}/utility/LOS/ && julia Config.jl --doCheckShadow no 
>>>cd ${PTDIR}/utility/LOS/src/ && julia main.jl 2015-02-11T00:00:00 TEST
>>>cd ${PTDIR}/utility/LOS/Tmp/output && cat *.dat > test_LOS-gas--shadow=no.res
>>>cd ${PTDIR}/utility/LOS/Tmp/output && rm *.dat

>>>@echo "test_LOS_run... done" >> test_LOS.diff

Check=
>>>@echo "test_LOS_check..." >> test_LOS.diff
>>>-@${SCRIPTDIR}/DiffNum.pl ${PTDIR}/utility/LOS/Tmp/output/test_LOS-dust--shadow=yes.res output/test_LOS/test_LOS-dust--shadow=yes.ref > test_LOS-dust--shadow=yes.diff 
>>>-@${SCRIPTDIR}/DiffNum.pl ${PTDIR}/utility/LOS/Tmp/output/test_LOS-dust--shadow=no.res output/test_LOS/test_LOS-dust--shadow=no.ref > test_LOS-dust--shadow=no.diff

>>>-@${SCRIPTDIR}/DiffNum.pl ${PTDIR}/utility/LOS/Tmp/output/test_LOS-gas--shadow=yes.res output/test_LOS/test_LOS-gas--shadow=yes.ref > test_LOS-gas--shadow=yes.diff
>>>-@${SCRIPTDIR}/DiffNum.pl ${PTDIR}/utility/LOS/Tmp/output/test_LOS-gas--shadow=no.res output/test_LOS/test_LOS-gas--shadow=no.ref > test_LOS-gas--shadow=no.diff

>>>cat test_LOS-dust--shadow=yes.diff test_LOS-dust--shadow=no.diff test_LOS-gas--shadow=yes.diff test_LOS-gas--shadow=no.diff > test_LOS.diff

>>>@ls -l test_LOS-dust--shadow=yes.diff 
>>>@ls -l test_LOS-dust--shadow=no.diff
>>>@ls -l test_LOS-gas--shadow=yes.diff
>>>@ls -l test_LOS-gas--shadow=no.diff
>>>@ls -l test_LOS.diff
>>>@echo Test test_LOS has finished: `date`
