Time=10

Ref=2{icc}@@(0+valeriy)
Ref=1{gcc}@@(0+valeriy)
Ref=8{pgcc}@@(0+valeriy)

Ref=5{gcc}@@(0+amps-gpu+passion)
Ref=9{icc}@@(0+amps-gpu+passion)
Ref=10{pgcc}@@(0+amps-gpu)

Ref=6{icc}@@(0+pleiades)
Ref=4{gcc}@@(0+pleiades)

Outs=

Keys=-link-option=./share/lib/libSHARE.a -compiler-option=-I${PTDIR}/share/Library/src/ -I${PTDIR}/srcInterface/

