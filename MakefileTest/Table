>>>-@utility/RemoveColumn.pl -c=5 SWMF-IH/SWMF/run_test/pic.H_PLUS.s=0.out=4.dat.Reduced.Step=500
>>>-@${SCRIPTDIR}/DiffNum.pl SWMF-IH/SWMF/run_test/pic.H_PLUS.s=0.out=4.dat.Reduced.Step=500 output/test_SEP3D/IH-KARTAVYKH-DRIFT.Step=500${TESTSEP--focused_transport-IH-Kartavykh-drift-REF}.ref > test_SEP--focused_transport-IH-Kartavykh-drift${TESTSEP--focused_transport-IH-Kartavykh-drift-REF}.diff

>>>@ls -l test_SEP--focused_transport-IH-Kartavykh-drift${TESTSEP--focused_transport-IH-Kartavykh-drift-REF}.diff
>>>@echo Test SEP--focused_transport-IH-Kartavykh-drift has finished: `date`
