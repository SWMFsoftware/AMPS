Type=swmf

Ref=2{icc}@@(0+valeriy)
Ref=1{gcc}@@(0+valeriy)

Compile=
>>>@echo "test_SEP--focused_transport-IH-Kartavykh-drift_diffusion_compile..." > test_SEP--focused_transport-IH-Kartavykh-drift_diffusion${TESTSEP--focused_transport-IH-Kartavykh-drift_diffusion-REF}.diff
>>>echo "Installing SWMF....."

<<<ifeq ($(wildcard SWMF-IH/*),)
>>>mkdir -p SWMF-IH
>>>cd SWMF-IH;git clone --depth 1 git@github.com:SWMFsoftware/SWMF
>>>ln -s ${PTDIR}/SWMF_data ${PTDIR}/SWMF-IH/SWMF/SWMF_data

<<<ifeq ($(COMPILE.c),icc)
>>>cd SWMF-IH/SWMF;./Config.pl  -sleep=10 -install -compiler=ifort,iccmpicxx
<<<endif

<<<ifeq ($(COMPILE.c),gcc)
>>>cd SWMF-IH/SWMF;./Config.pl  -sleep=10 -install -compiler=gfortran,gcc_mpicc
<<<endif

<<<ifeq ($(COMPILE.c),pgcc)
>>>cd SWMF-IH/SWMF;./Config.pl  -sleep=10 -install -compiler=pgf90,pgccmpicxx
<<<endif
<<<else
>>>rm -rf SWMF-IH/SWMF/run_test
<<<endif

>>>-cd SWMF-IH/SWMF;rm -rf run_test
>>>-cd SWMF-IH/SWMF;Scripts/Configure.pl -on=SP,SC,IH,PT -off=IM,IE,RB
>>>-cd SWMF-IH/SWMF;./Config.pl -v=Empty,SC/BATSRUS,IH/BATSRUS,SP/MFLAMPA,PT/AMPS
>>>-cd SWMF-IH/SWMF;./Config.pl -o=SC:u=AwsomR,e=Awsom,ng=2,g=4,4,4
>>>-cd SWMF-IH/SWMF;./Config.pl -o=IH:u=AwsomR,e=Awsom,ng=2,g=4,4,4
>>>-cd SWMF-IH/SWMF;./Config.pl -o=SP:g=2000
>>>-cd SWMF-IH/SWMF;./Config.pl -o=PT:spice-path=nospice,spice-kernels=nospice,application=test/sep_focused_transport-ih-mover_kartavykh_2016_aj_drift_diffusion,amps-test=on
>>>-cd SWMF-IH/SWMF;${MAKE} SWMF
>>>-cd SWMF-IH/SWMF;make PIDL

>>>@echo "test_SEP--focused_transport-IH-Kartavykh-drift_diffusion_compile... done" >> test_SEP--focused_transport-IH-Kartavykh-drift_diffusion${TESTSEP--focused_transport-IH-Kartavykh-drift_diffusion-REF}.diff

Rundir=
>>>@echo "test_SEP--focused_transport-IH-Kartavykh-drift_diffusion_rundir..." >> test_SEP--focused_transport-IH-Kartavykh-drift_diffusion.diff
>>>-rm -rf SWMF-IH/SWMF/run_test
>>>-cd SWMF-IH/SWMF;${MAKE} test15_rundir
>>>cp data/input/SEP/IH/PARAM.in SWMF-IH/SWMF/run_test
>>>@echo "test_SEP--focused_transport-IH-Kartavykh-drift_diffusion_rundir... done" >> test_SEP--focused_transport-IH-Kartavykh-drift_diffusion.diff

Run=
>>>@echo "test_SEP--focused_transport-IH-Kartavykh-drift_diffusion_run..." >> test_SEP--focused_transport-IH-Kartavykh-drift_diffusion.diff
>>>@echo Test test_SEP--focused_transport-IH-Kartavykh-drift_diffusion has started: `date`
>>>-cd SWMF-IH/SWMF/run_test;mpirun -np 8 ./SWMF.exe > runlog
>>>@echo "test_SEP--focused_transport-IH-Kartavykh-drift_diffusion_run... done" >> test_SEP--focused_transport-IH-Kartavykh-drift_diffusion.diff

Check=
>>>@echo "SEP--focused_transport-IH-Kartavykh-drift_diffusion_check..." >> test_SEP--focused_transport-IH-Kartavykh-drift_diffusion${TESTSEP--focused_transport-IH-Kartavykh-drift_diffusion-REF}.diff

>>>@utility/ReduceFile.pl SWMF-IH/SWMF/run_test/pic.H_PLUS.s=0.out=4.dat 500
>>>-@utility/RemoveColumn.pl -c=5 SWMF-IH/SWMF/run_test/pic.H_PLUS.s=0.out=4.dat.Reduced.Step=500
>>>-@${SCRIPTDIR}/DiffNum.pl SWMF-IH/SWMF/run_test/pic.H_PLUS.s=0.out=4.dat.Reduced.Step=500 output/test_SEP3D/IH-KARTAVYKH-DRIFT_diffusion.Step=500${TESTSEP--focused_transport-IH-Kartavykh-drift_diffusion-REF}.ref > test_SEP--focused_transport-IH-Kartavykh-drift_diffusion${TESTSEP--focused_transport-IH-Kartavykh-drift_diffusion-REF}.diff

