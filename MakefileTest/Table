Name=test/fast-wave
Time=3

Outs=

Keys=-link-option=./share/lib/libSHARE.a -compiler-option=-I${PTDIR}/share/Library/src/ -I${PTDIR}/srcInterface/

Compile=
>>>@echo "test_fast-wave_compile..." > test_fast-wave$(TESTfast-wave-REF).diff
>>>cd ./share/; mkdir -p lib
>>>cd ./share/Library/src; make LIB
>>>./Config.pl -application=test/fast-wave ${TESTfast-waveKEYS} 
>>>@rm -rf srcTemp
>>>-@($(MAKE) -j8  amps)
>>>@echo "test_fast-wave_compile... done" >> test_fast-wave$(TESTfast-wave-REF).diff


Check=
>>>@echo "test_fast-wave_check..." >> test_fast-wave$(TESTfast-wave-REF).diff

>>>mkdir -p output/test_fast-wave/tmp
>>>cp output/test_fast-wave/pic* output/test_fast-wave/tmp

>>>-@${TESTfast-waveDIR}/../utility/RemoveColumn.pl -c=5 output/test_fast-wave/pic.H_PLUS.s=0.out=2.ref_np`ls $(TESTfast-waveDIR)/PT/thread* |wc -l |tr -d ' '` 
>>>-@${TESTfast-waveDIR}/../utility/RemoveColumn.pl -c=5 output/test_fast-wave/pic.25THELECTRON.s=1.out=2.ref_np`ls $(TESTfast-waveDIR)/PT/thread* |wc -l |tr -d ' '`

>>>-@${TESTfast-waveDIR}/../utility/RemoveColumn.pl -c=5 output/test_fast-wave/pic.H_PLUS.s=0.out=2.CorrectionDivE.ref_np`ls $(TESTfast-waveDIR)/PT/thread* |wc -l |tr -d ' '`
>>>-@${TESTfast-waveDIR}/../utility/RemoveColumn.pl -c=5 output/test_fast-wave/pic.25THELECTRON.s=1.out=2.CorrectionDivE.ref_np`ls $(TESTfast-waveDIR)/PT/thread* |wc -l |tr -d ' '`

>>>-@${TESTfast-waveDIR}/../utility/RemoveColumn.pl -c=5 $(TESTfast-waveDIR)/PT/plots/pic.H_PLUS.s=0.out=2.dat
>>>-@${TESTfast-waveDIR}/../utility/RemoveColumn.pl -c=5 $(TESTfast-waveDIR)/PT/plots/pic.25THELECTRON.s=1.out=2.dat

>>>-(${SCRIPTDIR}/DiffNum.pl -a=2.0E-14 $(TESTfast-waveDIR)/PT/plots/pic.H_PLUS.s=0.out=2.dat \ 
>>>output/test_fast-wave/pic.H_PLUS.s=0.out=2.ref_np`ls $(TESTfast-waveDIR)/PT/thread* |wc -l |tr -d ' '`  > test_fast-wave--protons$(TESTfast-wave-REF).diff;)

>>>-(${SCRIPTDIR}/DiffNum.pl -a=2.0E-14 $(TESTfast-waveDIR)/PT/plots/pic.25THELECTRON.s=1.out=2.dat \ 
>>>output/test_fast-wave/pic.25THELECTRON.s=1.out=2.ref_np`ls $(TESTfast-waveDIR)/PT/thread* |wc -l |tr -d ' '`  > test_fast-wave--electrons$(TESTfast-wave-REF).diff)

>>>mv output/test_fast-wave/tmp/* output/test_fast-wave/
>>>rm -rf output/test_fast-wave/tmp

>>>mv $(TESTfast-waveDIR)/PT/plots/pic.H_PLUS.s=0.out=2.dat.tmp $(TESTfast-waveDIR)/PT/plots/pic.H_PLUS.s=0.out=2.dat
>>>mv $(TESTfast-waveDIR)/PT/plots/pic.25THELECTRON.s=1.out=2.dat.tmp $(TESTfast-waveDIR)/PT/plots/pic.25THELECTRON.s=1.out=2.dat

>>>@cat test_fast-wave--protons$(TESTfast-wave-REF).diff  test_fast-wave--electrons$(TESTfast-wave-REF).diff > test_fast-wave$(TESTfast-wave-REF).diff 

>>>@ls -l test_fast-wave--protons$(TESTfast-wave-REF).diff
>>>@ls -l test_fast-wave--electrons$(TESTfast-wave-REF).diff
>>>@ls -l test_fast-wave$(TESTfast-wave-REF).diff
>>>@echo Test test_fast-wave has finished: `date`

