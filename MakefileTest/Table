Name=test/SC-Cont@@(0+) 
Time=60 

Ref=2{icc}@@(0+valeriy)
Ref=1{gcc}@@(0+valeriy)
Ref=8{pgcc}@@(0+valeriy)

Ref=6{icc}@@(0+pleiades)

Keys=-cplr-data-path=/Users/vtenishe/AMPS_DATA_TEST/CYGNSS -model-data-path=/Users/vtenishe/AMPS_DATA_TEST/CYGNSS@@(0+valeriy) 
Keys=-cplr-data-path=/home/vtenishe/AMPS_DATA_TEST/CYGNSS -model-data-path=/home/vtenishe/AMPS_DATA_TEST/CYGNSS@@(0+amps-gpu+passion)
Keys=-cplr-data-path=/nobackupp8/vtenishe/AMPS_DATA_TEST/CYGNSS -model-data-path=/nobackupp8/vtenishe/AMPS_DATA_TEST/CYGNSS@@(0+pleiades)

Compile=
>>>@echo "test_SC-Cont_compile..." > test_SC-Cont$(TESTSC-Cont-REF).diff
>>>./Config.pl -application=test/sc-contamination -spice-path=nospice -spice-kernels=nospice -model-data-path=$(PTDIR)/data/input/SC-Cont  -amps-test=on $(TESTSC-ContKEYS)
>>>rm -rf srcTemp
>>>@($(MAKE) -j8 amps)
>>>@echo "test_SC-Cont_compile... done" >> test_SC-Cont$(TESTSC-Cont-REF).diff

Run=
>>>@echo "test_SC-Cont_run..." >> test_SC-Cont$(TESTSC-Cont-REF).diff
>>>@echo Test test_SC-Cont has started: `date`
>>>cd $(TESTSC-ContDIR); ${MPIRUN} ./amps
>>>@echo "test_SC-Cont_run... done" >> test_SC-Cont$(TESTSC-Cont-REF).diff

Check=
>>>@echo "test_SC-Cont_check..." >> test_SC-Cont$(TESTSC-Cont-REF).diff

>>>@${TESTSC-ContDIR}/../utility/ReduceFile.pl ${TESTSC-ContDIR}/PT/plots/SurfaceTriangulation.dat 10
>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTSC-ContDIR}/PT/plots/SurfaceTriangulation.dat.Reduced.Step=10 ${PTDIR}/../../../AMPS_DATA_TEST/CYGNSS/ContaminationTestOutput/SurfaceTriangulation.dat.Reduced.Step=10.ref_np`ls $(TESTSC-ContDIR)/PT/thread* |wc -l |tr -d ' '`  > test_SC-Cont-Triangulation.diff

>>>@${TESTSC-ContDIR}/../utility/ReduceFile.pl ${TESTSC-ContDIR}/PT/plots/VolumeMesh.dat 1000
>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTSC-ContDIR}/PT/plots/VolumeMesh.dat.Reduced.Step=1000 ${PTDIR}/../../../AMPS_DATA_TEST/CYGNSS/ContaminationTestOutput/VolumeMesh.dat.Reduced.Step=1000.ref_np`ls $(TESTSC-ContDIR)/PT/thread* |wc -l |tr -d ' '`  > test_SC-Cont-VolumeMesh.diff

>>>@${TESTSC-ContDIR}/../utility/ReduceFile.pl ${TESTSC-ContDIR}/PT/plots/amps.TrajectoryTracking.out=2.s=0.H2O.dat 10
>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTSC-ContDIR}/PT/plots/amps.TrajectoryTracking.out=2.s=0.H2O.dat.Reduced.Step=10 ${PTDIR}/../../../AMPS_DATA_TEST/CYGNSS/ContaminationTestOutput/amps.TrajectoryTracking.out=2.s=0.H2O.dat.Reduced.Step=10$(TESTSC-Cont-REF).ref_np`ls $(TESTSC-ContDIR)/PT/thread* |wc -l |tr -d ' '`  > test_SC-TrajectoryTracking$(TESTSC-Cont-REF).diff

