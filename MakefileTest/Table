Compile=
>>>@echo "test_CG-Rosina-Liouville--Trajectory_compile..." > test_CG-Rosina-Liouville--Trajectory$(TESTCG-Rosina-Liouville--Trajectory-REF).diff

>>>@mkdir -p ../TempSourceDir  
>>>@cd ../TempSourceDir;git clone git@github.com:SWMFsoftware/AMPS
>>>@cd ../TempSourceDir/AMPS;git checkout ROSINA-END-OF-MISSION

>>>@mv srcCG srcCG--original
>>>@mv ../TempSourceDir/AMPS/srcCG .

>>>@mv src src--original
>>>@mv ../TempSourceDir/AMPS/src .

>>>@rm -rf ../TempSourceDir


>>>./Config.pl -application=test/cg-end-of-mission-rosina-trajectory -amps-test=on $(TESTCG-Rosina-Liouville--TrajectoryKEYS)
>>>./Config.pl -amps-test=off
>>>-@($(MAKE) -j8 amps)
>>>./Config.pl -amps-test=on

>>>@rm -rf srcCG
>>>@mv srcCG--original srcCG

>>>@rm -rf src
>>>@mv src--original src

>>>@echo "test_CG-Rosina-Liouville--Trajectory_compile... done" >> test_CG-Rosina-Liouville--Trajectory$(TESTCG-Rosina-Liouville--Trajectory-REF).diff

Check= 
>>>@echo "test_CG-Rosina-Liouville--Trajectory_check..." >> test_CG-Rosina-Liouville--Trajectory$(TESTCG-Rosina-Liouville--Trajectory-REF).diff

>>>@${TESTCG-Rosina-Liouville--TrajectoryDIR}/../utility/ReduceFile.pl ${TESTCG-Rosina-Liouville--TrajectoryDIR}/PT/plots/SurfaceTriangulation.dat 40
>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTCG-Rosina-Liouville--TrajectoryDIR}/PT/plots/SurfaceTriangulation.dat.Reduced.Step=40   /Users/vtenishe/AMPS_DATA_TEST/ROSETTA/END-OF-MISSION/SurfaceTriangulation.dat.Reduced.Step=40$(TESTCG-Rosina-Liouville--Trajectory-REF).ref_np`ls $(TESTCG-Rosina-Liouville--TrajectoryDIR)/PT/thread* |wc -l |tr -d ' '`  > test_CG-Rosina-Liouville--Triangulation$(TESTCG-Rosina-Liouville--Trajectory-REF).diff

>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTCG-Rosina-Liouville--TrajectoryDIR}/PT/plots/RefTrajectory.dat  /Users/vtenishe/AMPS_DATA_TEST/ROSETTA/END-OF-MISSION/RefTrajectory$(TESTCG-Rosina-Liouville-Trajectory-REF).ref_np`ls $(TESTCG-Rosina-Liouville--TrajectoryDIR)/PT/thread* |wc -l |tr -d ' '`  > test_CG-Rosina-Liouville--RefTrajectory$(TESTCG-Rosina-Liouville--Trajectory-REF).diff

>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTCG-Rosina-Liouville--TrajectoryDIR}/PT/plots/RefMeasurements.dat  /Users/vtenishe/AMPS_DATA_TEST/ROSETTA/END-OF-MISSION/RefMeasurements$(TESTCG-Rosina-Liouville-Trajectory-REF).ref_np`ls $(TESTCG-Rosina-Liouville--TrajectoryDIR)/PT/thread* |wc -l |tr -d ' '`  > test_CG-Rosina-Liouville--RefMeasurements$(TESTCG-Rosina-Liouville--Trajectory-REF).diff

>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTCG-Rosina-Liouville--TrajectoryDIR}/PT/plots/RefNiceGroundTrack0.dat  /Users/vtenishe/AMPS_DATA_TEST/ROSETTA/END-OF-MISSION/RefNiceGroundTrack0$(TESTCG-Rosina-Liouville-Trajectory-REF).ref_np`ls $(TESTCG-Rosina-Liouville--TrajectoryDIR)/PT/thread* |wc -l |tr -d ' '`  > test_CG-Rosina-Liouville--RefGroundTrack$(TESTCG-Rosina-Liouville--Trajectory-REF).diff


>>>@ls -l test_CG-Rosina-Liouville--Triangulation$(TESTCG-Rosina-Liouville--Trajectory-REF).diff
>>>@ls -l test_CG-Rosina-Liouville--RefTrajectory$(TESTCG-Rosina-Liouville--Trajectory-REF).diff
>>>@ls -l test_CG-Rosina-Liouville--RefMeasurements$(TESTCG-Rosina-Liouville--Trajectory-REF).diff
>>>@ls -l test_CG-Rosina-Liouville--RefGroundTrack$(TESTCG-Rosina-Liouville--Trajectory-REF).diff
>>>@echo Test test_CG-Rosina-Liouville--Trajectory has finished: `date`

