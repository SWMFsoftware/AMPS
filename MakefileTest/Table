Name=SEP--focused_transport-IH-borovikov
Time=4
Type=swmf

Ref=2{icc}@@(0+valeriy)
Ref=1{gcc}@@(0+valeriy)

Compile=
>>>@echo "test_SEP--focused_transport-IH-borovikov_compile..." > test_SEP--focused_transport-IH-borovikov${TESTSEP--focused_transport-IH-borovikov-REF}.diff
>>>echo "Installing SWMF....."

<<<ifeq ($(wildcard SWMF-IH/*),)
>>>mkdir -p SWMF-IH
>>>cd SWMF-IH;git clone --depth 1 git@github.com:SWMFsoftware/SWMF
>>>ln -s ${PTDIR}/SWMF_data ${PTDIR}/SWMF-IH/SWMF/SWMF_data

<<<ifeq ($(COMPILE.c),icc)
>>>cd SWMF-IH/SWMF;./Config.pl  -sleep=10 -install -compiler=ifort,iccmpicxx
<<<endif

<<<ifeq ($(COMPILE.c),gcc)
>>>cd SWMF-IH/SWMF;./Config.pl  -sleep=10 -install -compiler=gfortran,gcc_mpicc
<<<endif

<<<ifeq ($(COMPILE.c),pgcc)
>>>cd SWMF-IH/SWMF;./Config.pl  -sleep=10 -install -compiler=pgf90,pgccmpicxx
<<<endif
<<<else
>>>rm -rf SWMF-IH/SWMF/run_test 
<<<endif

>>>-cd SWMF-IH/SWMF;rm -rf run_test
>>>-cd SWMF-IH/SWMF;Scripts/Configure.pl -on=SP,SC,IH,PT -off=IM,IE,RB
>>>-cd SWMF-IH/SWMF;./Config.pl -v=Empty,SC/BATSRUS,IH/BATSRUS,SP/MFLAMPA,PT/AMPS
>>>-cd SWMF-IH/SWMF;./Config.pl -o=SC:u=AwsomR,e=Awsom,ng=2,g=4,4,4
