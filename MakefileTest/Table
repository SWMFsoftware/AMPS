Type=swmf

ExeptionCode=SKIP{pgcc}

Compile=
>>>@echo "test_AMPS-BATSRUS-PIC-coupling_compile..." > test_AMPS-BATSRUS-PIC-coupling.diff
>>>echo "Installing SWMF....."

<<<ifeq ($(wildcard SWMF/*),)
>>>git clone --depth 1 git@github.com:SWMFsoftware/SWMF 
>>>ln -s ${PTDIR}/SWMF_data ${PTDIR}/SWMF/SWMF_data

<<<ifeq ($(COMPILE.c),icc)
>>>cd SWMF;./Config.pl  -sleep=10 -install -compiler=ifort,iccmpicxx 
<<<endif

<<<ifeq ($(COMPILE.c),gcc)
>>>cd SWMF;./Config.pl  -sleep=10 -install -compiler=gfortran,gcc_mpicc
<<<endif

<<<ifeq ($(COMPILE.c),pgcc)
>>>cd SWMF;./Config.pl  -sleep=10 -install -compiler=pgf90,pgccmpicxx
<<<endif
<<<endif

>>>@echo "test_AMPS-BATSRUS-PIC-coupling_compile... done" > test_AMPS-BATSRUS-PIC-coupling.diff

