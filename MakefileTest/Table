>>>@ls -l test_Periodic-BC--particle.diff

>>>@cat test_Periodic-BC--test=0-wave.diff test_Periodic-BC--test=1-wave.diff test_Periodic-BC--test=2-wave.diff test_Periodic-BC--test=3-wave.diff test_Periodic-BC--particle.diff > test_Periodic-BC.diff 
