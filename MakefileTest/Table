>>>-@${SCRIPTDIR}/DiffNum.pl -a=4.0E-13 ${TESTSC-ContDIR}/PT/plots/pic.H2O.s=0.out=2.dat.Reduced.Step=1000 ${PTDIR}/../../../AMPS_DATA_TEST/CYGNSS/ContaminationTestOutput/pic.H2O.s=0.out=2.dat.Reduced.Step=1000$(TESTSC-Cont-REF).ref_np`ls $(TESTSC-ContDIR)/PT/thread* |wc -l |tr -d ' '`  > test_SC-Volume-Sample$(TESTSC-Cont-REF).diff

